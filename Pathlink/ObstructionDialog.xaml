<Window x:Class="Pathlink.ObstructionDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Obstáculo" Height="200" Width="300">
    <StackPanel Margin="10">
        <ComboBox x:Name="ObstructionTypeComboBox" Margin="10">
            <ComboBoxItem Content="Vegetação"/>
            <ComboBoxItem Content="Construção"/>
        </ComboBox>
        <TextBox x:Name="ObstructionHeightTextBox" Margin="10" Text="" TextAlignment="Center" PreviewTextInput="TextBox_PreviewTextInput"/>
        <TextBox x:Name="ObstructionGrowthMarginTextBox" Margin="10" Text="" TextAlignment="Center" PreviewTextInput="TextBox_PreviewTextInput"/>
        <Button Content="Inserir" Click="InsertButton_Click" Margin="10"/>
    </StackPanel>
</Window>
